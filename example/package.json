{
  "name": "jestpack-example",
  "version": "0.0.1",
  "description": "Example usage of Jest Webpack",
  "main": "webpack.config.js",
  "scripts": {
    "start": "npm run build && npm test",
    "build": "node node_modules/.bin/webpack",
    "test": "node node_modules/.bin/jest --runInBand",
    "build-debug": "node-debug node_modules/.bin/webpack",
    "test-debug": "node-debug --nodejs --harmony node_modules/.bin/jest --runInBand"
  },
  "engines": {
    "node": "0.8.x || 0.10.x || >=2"
  },
  "author": "",
  "license": "ISC",
  "jest": {
    "moduleLoader": "<rootDir>/node_modules/jestpack/ModuleLoader",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/css-loader",
      "<rootDir>/node_modules/bundle-loader"
    ]
  },
  "devDependencies": {
    "babel-core": "^5.8.23",
    "babel-loader": "^5.3.2",
    "css-loader": "^0.17.0",
    "glob": "^5.0.14",
    "jest-cli": "^0.4.19 || ^0.5",
    "jestpack": "git+ssh://git@github.com:60frames/jestpack.git",
    "stats-webpack-plugin": "^0.2.1",
    "style-loader": "^0.12.3",
    "webpack": "^1.12.1"
  },
  "dependencies": {
    "lodash": "^3.10.1",
    "react": "^0.13.3"
  }
}
